og(key, keyPath);\r\n    };\r\n    return {\r\n      isCollapse,\r\n      handleOpen,\r\n      handleClose,\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    changeIsCollapse() {\r\n      if (this.isCollapse == 0) {\r\n        this.isCollapse = 1;\r\n      } else {\r\n        this.isCollapse = 0;\r\n      }\r\n    },\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      studentId: 200000,\r\n      tableData: [\r\n        {\r\n          courseId: 60002,\r\n          courseInfo: {\r\n            college: \"软件学院\",\r\n            credit: 2,\r\n            name: \"计算机组成原理\",\r\n            open_period: \"星期三10-11节 [1-17]\",\r\n            place: \"济事楼419\",\r\n            teacherInfo: Array(0),\r\n            length: 0,\r\n          },\r\n          isActive: 1,\r\n        },\r\n      ],\r\n    };\r\n  },\r\n\r\n  mounted: function () {\r\n    this.axios\r\n      .get(\"http://139.224.65.105:9090/api/student/takes/\" + this.studentId)\r\n      .then((response) => {\r\n        console.log(response.data);\r\n        if (response.data.code == 200) {\r\n          this.tableData = response.data.data;\r\n          return true;\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n  },\r\n});\r\n</script>\r\n"]}]}